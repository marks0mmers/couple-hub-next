{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "next/core-web-vitals"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "indent": ["warn", 2],
    "object-curly-spacing": ["warn", "always"],
    "semi": ["error", "always"],
    "comma-dangle": ["warn", "always-multiline"],
    "quotes": ["warn", "double"]
  }
}
